{
  "name": "@bedrock-ui/calendar",
  "version": "0.0.2",
  "description": "Bedrock UI Calendar",
  "files": [
    "lib"
  ],
  "module": "lib/esm/index.js",
  "main": "lib/cjs/index.js",
  "types": "lib/cjs/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm rm:build && pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json && tsc-alias -p tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json && tsc-alias -p tsconfig.build-esm.json",
    "build-storybook": "build-storybook",
    "dev": "concurrently \"npm run storybook\"",
    "rm:build": "rimraf lib",
    "storybook": "start-storybook -p 6006",
    "typecheck": "tsc --noEmit"
  },
  "author": "Matthew Wolfe",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/client-api": "^6.5.13",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@types/react": "^18.0.24",
    "babel-loader": "^9.0.1",
    "bytes": "^3.1.2",
    "concurrently": "^7.5.0",
    "fs-extra": "^10.1.0",
    "gzip-size": "^7.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  }
}
